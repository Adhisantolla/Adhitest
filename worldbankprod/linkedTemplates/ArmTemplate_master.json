{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "worldbankprod"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"LS_covid_CosmosDb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_covid_CosmosDb'"
		},
		"LS_worldbank_prodserver_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_worldbank_prodserver'"
		},
		"ECDC server_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://opendata.ecdc.europa.eu/covid19/"
		},
		"trigger1_properties_PL_copy_eventbase_parameters_filename": {
			"type": "string",
			"defaultValue": "@triggerbody().filename"
		},
		"trigger1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4213c321-262b-441d-a4f1-a4c221ea0385/resourceGroups/project_practice/providers/Microsoft.Storage/storageAccounts/worldbankraw"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "worldbankprod_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"LS_covid_CosmosDb_connectionString": {
						"value": "[parameters('LS_covid_CosmosDb_connectionString')]"
					},
					"LS_worldbank_prodserver_connectionString": {
						"value": "[parameters('LS_worldbank_prodserver_connectionString')]"
					},
					"ECDC server_properties_typeProperties_url": {
						"value": "[parameters('ECDC server_properties_typeProperties_url')]"
					},
					"trigger1_properties_PL_copy_eventbase_parameters_filename": {
						"value": "[parameters('trigger1_properties_PL_copy_eventbase_parameters_filename')]"
					},
					"trigger1_properties_typeProperties_scope": {
						"value": "[parameters('trigger1_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}